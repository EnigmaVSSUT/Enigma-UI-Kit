{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20.8.1"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "release": "npm run build &&  npx changeset publish",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "tsdx lint",
    "prepare": "tsdx build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "semantic-release": "npx semantic-release --branches release"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "enigma-ui-kit",
  "author": "SySagar",
  "module": "dist/enigma-ui-kit.esm.js",
  "size-limit": [
    {
      "path": "dist/enigma-ui-kit.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/enigma-ui-kit.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@size-limit/preset-small-lib": "^10.0.2",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "7.5.3",
    "@storybook/testing-react": "^2.0.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "eslint": "^6.8.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "semantic-release": "^23.0.0",
    "size-limit": "^10.0.2",
    "storybook": "^7.5.1",
    "tailwindcss": "^3.3.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EnigmaVSSUT/Enigma-UI-Kit.git"
  }
